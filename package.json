{
  "name": "project-auto-set",
  "displayName": "SIRe Project AutoSet ",
  "description": "Project Auto Set for VS Code",
  "version": "1.0.0",
  "publisher": "scarlet-ivory",
  "engines": {
    "vscode": "^1.90.0"
  },
  "repository": {
    "url": "https://github.com/scarletIvory/ReComponentsLib"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reComponentsLib.deleteFunc",
        "title": "Delete Function"
      },
      {
        "command": "reComponentsLib.createComponent",
        "title": "All"
      },
      {
        "command": "reComponentsLib.setProject",
        "title": "Set Project"
      },
      {
        "command": "reComponentsLib.generateComponent",
        "title": "Generate Component"
      },
      {
        "command": "reComponentsLib.createReTable",
        "title": "ReTable"
      },
      {
        "command": "reComponentsLib.createReDialog",
        "title": "ReDialog"
      },
      {
        "command": "reComponentsLib.createReTitleTab",
        "title": "ReTitleTab"
      },
      {
        "command": "reComponentsLib.createReSearchBar",
        "title": "ReSearchBar"
      },
      {
        "command": "reComponentsLib.openSettings",
        "title": "Open Settings"
      }
    ],
    "keybindings": [
      {
        "command": "reComponentsLib.deleteFunc",
        "key": "ctrl+f5",
        "when": "editorTextFocus",
        "mac": "cmd+f5"
      },
      {
        "command": "reComponentsLib.createComponent",
        "key": "ctrl+f6",
        "when": "explorerResourceIsFolder",
        "mac": "cmd+f6"
      },
      {
        "command": "reComponentsLib.setProject",
        "key": "ctrl+f7",
        "when": "explorerResourceIsFolder",
        "mac": "cmd+f7"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "reComponentsLib.deleteFunc",
          "when": "editorTextFocus",
          "group": "4_execution"
        },
        {
          "command": "reComponentsLib.generateComponent",
          "when": "editorTextFocus",
          "group": "4_execution"
        },
        {
          "command": "reComponentsLib.openSettings",
          "when": "editorTextFocus",
          "group": "4_execution"
        }
      ],
      "explorer/context": [
        {
          "submenu": "createComponent",
          "when": "explorerResourceIsFolder",
          "group": "4_new"
        },
        {
          "command": "reComponentsLib.setProject",
          "when": "explorerResourceIsFolder",
          "group": "4_new"
        }
      ],
      "createComponent": [
        {
          "command": "reComponentsLib.createComponent",
          "title": "All",
          "group": "1_new"
        },
        {
          "command": "reComponentsLib.createReTable",
          "title": "ReTable",
          "group": "2_new"
        },
        {
          "command": "reComponentsLib.createReDialog",
          "title": "ReDialog",
          "group": "2_new"
        },
        {
          "command": "reComponentsLib.createReTitleTab",
          "title": "ReTitleTab",
          "group": "2_new"
        },
        {
          "command": "reComponentsLib.createReSearchBar",
          "title": "ReSearchBar",
          "group": "2_new"
        }
      ]
    },
    "submenus": [
      {
        "id": "createComponent",
        "label": "Create Component"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "reComponentsLib Plugin Configuration",
      "properties": {
        "reComponentsLib.useScss": {
          "type": "boolean",
          "default": true,
          "description": "Enable SCSS"
        },
        "reComponentsLib.scssVersion": {
          "type": "string",
          "default": "^1.32.8",
          "description": "SCSS Version"
        },
        "reComponentsLib.useLess": {
          "type": "boolean",
          "default": true,
          "description": "Enable LESS"
        },
        "reComponentsLib.lessVersion": {
          "type": "string",
          "default": "^4.1.1",
          "description": "LESS Version"
        },
        "reComponentsLib.usePxToRem": {
          "type": "boolean",
          "default": true,
          "description": "Enable px to rem conversion"
        },
        "reComponentsLib.pxToRemVersion": {
          "type": "string",
          "default": "^5.1.1",
          "description": "px to rem Version"
        },
        "reComponentsLib.useESLint": {
          "type": "boolean",
          "default": true,
          "description": "Enable ESLint"
        },
        "reComponentsLib.eslintPackages": {
          "type": "object",
          "description": "ESLint Packages",
          "properties": {
            "@typescript-eslint/eslint-plugin": {
              "type": "string",
              "default": "^7.7.1",
              "description": "@typescript-eslint/eslint-plugin Version"
            },
            "@typescript-eslint/parser": {
              "type": "string",
              "default": "^7.7.1",
              "description": "@typescript-eslint/parser Version"
            },
            "eslint": {
              "type": "string",
              "default": "^8.57.0",
              "description": "ESLint Version"
            },
            "eslint-plugin-import": {
              "type": "string",
              "default": "^2.20.2",
              "description": "eslint-plugin-import Version"
            },
            "eslint-plugin-node": {
              "type": "string",
              "default": "^11.1.0",
              "description": "eslint-plugin-node Version"
            },
            "eslint-plugin-promise": {
              "type": "string",
              "default": "^4.2.1",
              "description": "eslint-plugin-promise Version"
            },
            "eslint-plugin-standard": {
              "type": "string",
              "default": "^4.0.0",
              "description": "eslint-plugin-standard Version"
            },
            "eslint-plugin-vue": {
              "type": "string",
              "default": "^9.25.0",
              "description": "eslint-plugin-vue Version"
            }
          },
          "default":{
            "@typescript-eslint/eslint-plugin": "^7.7.1",
            "@typescript-eslint/parser": "^7.7.1",
            "eslint": "^8.57.0",
            "eslint-plugin-import": "^2.20.2",
            "eslint-plugin-node": "^11.1.0",
            "eslint-plugin-promise": "^4.2.1",
            "eslint-plugin-standard": "^4.0.0",
            "eslint-plugin-vue": "^9.25.0"
          },
          "additionalProperties": false
        },
        "reComponentsLib.useStylelint": {
          "type": "boolean",
          "default": true,
          "description": "Enable Stylelint"
        },
        "reComponentsLib.stylelintPackages": {
          "type": "object",
          "description": "Stylelint Packages",
          "default": {
            "stylelint": "^13.13.1",
            "stylelint-config-standard": "^20.0.0",
            "stylelint-config-recommended-vue": "^1.3.0"
          },
          "properties": {
            "stylelint": {
              "type": "string",
              "default": "^13.13.1",
              "description": "Stylelint Version"
            },
            "stylelint-config-standard": {
              "type": "string",
              "default": "^20.0.0",
              "description": "stylelint-config-standard Version"
            },
            "stylelint-config-recommended-vue": {
              "type": "string",
              "default": "^1.3.0",
              "description": "stylelint-config-recommended-vue Version"
            }
          },
          "additionalProperties": false
        },
        "reComponentsLib.usePinia": {
          "type": "boolean",
          "default": false,
          "description": "Enable Pinia"
        },
        "reComponentsLib.piniaVersion": {
          "type": "string",
          "default": "^2.0.0",
          "description": "Pinia Version"
        },
        "reComponentsLib.useElementPlus": {
          "type": "boolean",
          "default": false,
          "description": "Enable Element Plus"
        },
        "reComponentsLib.elementPlusVersion": {
          "type": "string",
          "default": "^2.2.1",
          "description": "Element Plus Version"
        },
        "reComponentsLib.usePnpm": {
          "type": "boolean",
          "default": false,
          "description": "Enable pnpm configuration"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.37",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@babel/parser": "^7.24.7",
    "@babel/traverse": "^7.24.7",
    "@types/jsdom": "^21.1.7",
    "@vue/compiler-sfc": "^3.4.29",
    "jsdom": "^24.1.0",
    "ts-node": "^10.9.2"
  }
}
